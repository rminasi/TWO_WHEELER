<div class="container-found-page">
  <div class="found-something-cont-image">
    <img src="https://img.icons8.com/color/96/000000/cycling-track--v2.png">
  </div>
    <div class="found-something-cont-form">
      <span>Log in</span>
        <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="form-inputs">
        <%= f.input :email,
                    required: false,
                    autofocus: true,
                    input_html: { autocomplete: "email" } %>
        <%= f.input :password,
                    required: false,
                    input_html: { autocomplete: "current-password" } %>
        <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
        </div>

        <div class="form-actions">
        <%= f.button :submit, "Log in", class: 'btn btn-success' %>
        </div>
        <% end %>

      <%= render "devise/shared/links" %>
    </div>
</div>
