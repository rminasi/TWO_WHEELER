
<div class="main-container">
  <div class="list-bike">
    <div class="col-md-8">
      <h1>List your bike!</h1>
      <%= simple_form_for(@bike) do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>
        <%= f.input :location %>
        <%= f.input :image %>
        <%= f.button :submit, class: 'btn btn-success'%>
      <% end %>
    </div>
  </div>
</div>
